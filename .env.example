# DMP 饥荒管理平台机器人配置文件示例
# 复制此文件为 .env 并填入实际配置

# DMP 服务器配置
DMP_BASE_URL=http://your-dmp-server:port/v1
DMP_TOKEN=your-jwt-token-here
DEFAULT_CLUSTER=clustername

# OneBot 配置
ONEBOT_WS_URLS=["ws://your-onebot-server:port"]
ONEBOT_ACCESS_TOKEN=your-access-token-here

# 超级用户配置
SUPERUSERS=["你的QQ号"]

# 机器人昵称
NICKNAME=["DMP机器人"]

# 命令前缀
COMMAND_START=["/"]
COMMAND_SEP=["."]

# 日志配置
NONEBOT_LOG_LEVEL=INFO
NONEBOT_DEBUG=false

# 会话超时配置
SESSION_EXPIRE_TIMEOUT=120
